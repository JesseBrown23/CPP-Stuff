#include <iostream>

using namespace std;

void printNumber(int x);
void printNumber(float x);

int main()
{
    int a = 54;
    float b = 32.4;

    printNumber(a);
    printNumber(b);

    return 0;
}

void printNumber(int x){
    cout << "I am printing a int: " << x << endl;
}

void printNumber(float x){
    cout << "I am printing a float: " << x << endl;
}
